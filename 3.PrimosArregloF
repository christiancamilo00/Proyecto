SubProceso  Primos(dato)
	
	definir num,n,i,respuesta como entero;
	respuesta <- dato;
	
	para n <- 1 hasta 1000 Con Paso 1 Hacer
		
		
		num<-0;
		para i<-1 hasta n Hacer
			si n%i=0 Entonces
				num<-num +1;
			FinSi
		FinPara
		
		si num=2 Entonces
			Escribir n, " Es primo";
		sino 
			Escribir n, " No es primo";
		FinSi
		
	FinPara
	
FinSubProceso

Proceso PrimosF
	Definir dato Como Entero;
	dato <- 0;
	Primos(dato);
	
FinProceso