Proceso ArbolCicloRepetir
	Definir control Como logico;
	Definir incremento Como Entero;
	Definir incremento1 Como Entero;
	Definir incremento2 Como Entero;
	control  <- Verdadero;
	incremento  <- 1;
	incremento1  <- 10;
	
	Repetir
		Escribir "-",Sin Saltar;
		incremento  <- incremento + 1;
		Repetir
			Escribir "*",Sin Saltar;
			incremento1  <- incremento1 - 1;
			
		Hasta Que incremento1 = 1;
		
	Hasta Que incremento = 10;
	
FinProceso